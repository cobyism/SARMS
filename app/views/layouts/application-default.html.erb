<!DOCTYPE html>
<html>
<head>
  <title>SARMS</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

  <div id="nav">
    <%= link_to "Home", root_path %> |
    <%= link_to "Users", users_path %> |
    <%= link_to "Units", units_path %> |
    <%= link_to "Enrollments", enrollments_path %> |
    <%= link_to "Faculty Assignments", faculty_assignments_path %> |
    <%= link_to "Assessments", assessments_path %> |
    <%= link_to "Activities", activities_path %> |
    <%= link_to "Attendance", attendances_path %> |
    <%= link_to "Performance", performances_path %> |
    <%= link_to "Notes", notes_path %> |
    <%= link_to "At Risk Events", at_risk_events_path %> |
    <%= link_to "Responses", responses_path %> |
<%#   </div>%>

<%# <div id="user_nav">%>
  <% if current_user%>
    Signed in as <%= current_user.email %> |
    <%= link_to "Sign out", sign_out_path %>
  <% else%>
    <%# link_to "Sign in", sign_in_path %>
  <% end%>
</div>

<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash_#{name}" %>
<% end %>

<%= yield %>

</body>
</html>
